{
  "manifest_version": 2,

  "name": "URL QR code",
  "description": "Create a QR code of the url you are seeing now",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "icons": { 
  "128": "icon_128.png" 
  },
  "permissions": [
    "tabs"
  ]
}
